{
  "name": "workerman/webman",
  "type": "project",
  "description": "High performance HTTP Service Framework.",
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "ext-pdo": "*",
    "cornyk/webman-beanstalk-queue": "^0.1",
    "doctrine/annotations": "^1.14",
    "guzzlehttp/guzzle": "^7.8",
    "illuminate/database": "^11.13",
    "illuminate/events": "^11.13",
    "illuminate/pagination": "^11.13",
    "illuminate/redis": "^11.13",
    "monolog/monolog": "^2.0",
    "php-di/php-di": "^6",
    "psr/container": "^1.1.1",
    "respect/validation": "^2.3",
    "vlucas/phpdotenv": "^5.6",
    "webman/console": "^1.3",
    "webman/event": "^1.0",
    "workerman/webman-framework": "^1.5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10"
  },
  "suggest": {
    "ext-event": "For better performance. "
  },
  "autoload": {
    "psr-4": {
      "": "./",
      "app\\": "./app"
    },
    "files": [
      "./support/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "./tests"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "scripts": {
    "post-package-install": [
      "support\\Plugin::install"
    ],
    "post-package-update": [
      "support\\Plugin::install"
    ],
    "pre-package-uninstall": [
      "support\\Plugin::uninstall"
    ],
    "pack": "@php -d phar.readonly=0 ./webman phar:pack",
    "build": "@php -d phar.readonly=0 ./webman build:bin 8.2"
  }
}
